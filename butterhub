local Tab = Window:NewTab("Settings")
local Section = Tab:NewSection("UI")
Section:NewKeybind("Toggle UI", "beepbeep", Enum.KeyCode.RightShift, function()
	Library:ToggleUI()
end)
Section:NewSlider("WalkSpeed", "beepbeep", 250, 0, function(s) -- 500 (MaxValue) | 0 (MinValue)
    game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = s
end)
Section:NewSlider("JumpPowa", "beepbeep", 250, 0, function(s) -- 500 (MaxValue) | 0 (MinValue)
    game.Players.LocalPlayer.Character.Humanoid.JumpPower = s
end)
